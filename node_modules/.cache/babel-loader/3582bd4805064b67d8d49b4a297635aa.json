,MAMA;AACA,QAAA,KAAA,CAAA,eAAA,CAAA;AACA;AACA,KA5BA;AA6BA,GAvCA;AAwCA,EAAA,OAAA,EAAA;AACA,IAAA,mBADA,+BACA,GADA,EACA;AACA,WAAA,UAAA,GAAA,GAAA;AACA,KAHA;AAIA,IAAA,OAJA,qBAKA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AACA,KAPA;AAQA,IAAA,UARA,wBASA;AACA,UAAA,KAAA,UAAA,IAAA,IAAA,EACA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA,YAAA;AAAA,UAAA,MAAA,EAAA;AAAA,YAAA,UAAA,EAAA,KAAA;AAAA;AAAA,SAAA;AACA;AACA,KAdA;AAeA,IAAA,UAfA,wBAgBA;AAAA;;AACA,UAAA,KAAA,UAAA,IAAA,IAAA,EACA;AACA,aAAA,QAAA,CAAA,0CAAA,KAAA,UAAA,CAAA,KAAA,EAAA,QAAA,EAAA;AACA,UAAA,iBAAA,EAAA,SADA;AAEA,UAAA,gBAAA,EAAA,QAFA;AAGA,UAAA,IAAA,EAAA;AAHA,SAAA,EAIA,IAJA,CAIA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AAAA,YAAA,IAAA,EAAA,SAAA;AAAA,YAAA,OAAA,EAAA;AAAA,WAAA;AACA,SANA,EAMA,KANA,CAMA,YAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,MADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SAXA;AAYA;AACA;AAhCA;AAxCA,CAAA","sourcesContent":["<template>\n  <div class=\"usermanagement\">\n    <el-table\n      ref=\"singleTable\"\n      :data=\"tableData\"\n      highlight-current-row\n      @current-change=\"handleCurrentChange\"\n      style=\"width: 100%;\">\n      <el-table-column\n        type=\"index\"\n        min-width=\"9%\">\n      </el-table-column>\n      <el-table-column\n        property=\"email\"\n        label=\"E-mail\"\n        min-width=\"14%\">\n      </el-table-column>\n      <el-table-column\n        property=\"totalstorage\"\n        label=\"Total Storage\"\n        min-width=\"14%\">\n      </el-table-column>\n      <el-table-column\n        property=\"usedstorage\"\n        label=\"Used Storage\"\n        min-width=\"14%\">\n      </el-table-column>\n      <el-table-column\n        property=\"blockedstatus\"\n        label=\"Blocked Status\"\n        min-width=\"10%\">\n      </el-table-column>\n      <el-table-column\n        property=\"uploadaccess\"\n        label=\"Upload Access\"\n        min-width=\"10%\">\n      </el-table-column>\n      <el-table-column\n        property=\"downloadaccess\"\n        label=\"Download Access\"\n        min-width=\"10%\">\n      </el-table-column>\n      <el-table-column\n        property=\"sourcetype\"\n        label=\"Source Type\"\n        min-width=\"14%\">\n      </el-table-column>\n    </el-table>\n    <div style=\"margin-top: 20px\">\n      <el-button style=\"float:left;margin-left: 3%\" @click=\"adduser\">Add a new user</el-button>\n      <el-button style=\"float:left\" @click=\"changeuser\">Change user config</el-button>\n      <el-button style=\"float:right;margin-right: 3%\" @click=\"deleteuser\">Delete a user</el-button>\n    </div>\n  </div>\n\n</template>\n\n<script>\n\nexport default {\n  name: 'Usermanagement',\n  data() {\n    return {\n      tableData: [],\n      currentRow: null\n    }\n  },\n  mounted: function () \n  {\n    this.$api.get('/admin/users', \n    {\n      'x-auth-token': this.Common.xtoken\n    }, \n    {\n    }, response => {\n    if (response.status == 200) \n    {\n      var userslist;\n\n      userslist = response.data.result_data.users;\n      for (var key in userslist)\n      {\n        this.tableData.push({email: userslist[key].email,totalstorage: userslist[key].total_storage,usedstorage: userslist[key].used_storage,\n                            blockedstatus: userslist[key].blocked_status.toString(),uploadaccess: userslist[key].upload_access.toString(),\n                            downloadaccess: userslist[key].download_access.toString(),sourcetype: userslist[key].source_type});\n      }\n      \n    } \n    else if(response.status == 400)\n    {\n      alert(response.data.msg);\n      \n    }\n    else\n    {\n      alert(\"Network Error\");\n    }\n    });\n  },\n    methods:{\n    handleCurrentChange(val) {\n      this.currentRow = val;\n    },\n    adduser () \n    {\n      this.$router.push({name:\"Useradd\"})\n    },\n    changeuser () \n    {\n      if(this.currentRow != null)\n      {\n        this.$router.push({name:\"Userchange\",params: {changeitem: this.currentRow}})\n      }\n    },\n    deleteuser ()\n    {\n      if(this.currentRow != null)\n      {\n        this.$confirm('This operation will delete this user:'+this.currentRow.email, 'Delete', {\n          confirmButtonText: 'Confirm',\n          cancelButtonText: 'Cancel',\n          type: 'warning'\n        }).then(() => {\n          this.$message({type: 'success',message: 'Success!'});\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: 'Canceled'\n          });          \n        });\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\n.usermanagement\n{\n  background-color:white;\n  height:100%;\n}\n</style>\n"],"sourceRoot":"src/components/Adminsubpage"}]}