>\n      <el-main>\n        <el-input v-model=\"password\" placeholder=\"Password\"></el-input>\n      </el-main>\n      <el-main>\n        <el-button type=\"primary\" style=\"float:right;width:25%;font-size:1.3em;\" @click=\"UserLogin\">Next</el-button>\n      </el-main>\n    </el-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Login',\n  data () {\n    return{\n    username:'',\n    password:''\n    }\n  },\n  methods:{\n    UserLogin () {\n      this.$api.post('/api/login', \n      {\n        username: this.username,                        //'johnwick123@gmail.com',\n        password: this.password                       //'1231234'\n      }, response => {\n      if (response.status == 200) \n      {\n        this.$router.push({name:\"MainFrame\"})\n      } \n      else if(response.status == 400)\n      {\n        alert(response.data.msg);\n        \n      }\n      else\n      {\n        alert(\"Network Error\");\n      }\n      });\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style>\n/* .login{background:url(../assets/LoginWallpaper.jpeg)} */\n.login{background-color:#EBEEF5}\n.loginput{\n  background-color:white; \n  height: 440px;\n  width:450px;\n  position:absolute;\n  left:50%;\n  top:50%;\n  transform:translate(-50%,-50%);\n  border-radius: 6px;\n  box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04);\n  }\n</style>\n"],"sourceRoot":"src/components"}]}